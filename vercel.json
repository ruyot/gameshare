{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "client.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/signaling",
      "dest": "/server.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/client.js",
      "dest": "/client.js"
    }
  ],
  "functions": {
    "server.js": {
      "maxDuration": 30
    }
  }
} 